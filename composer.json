{
    "name": "orbitale/compotes",
    "type": "project",
    "license": "AGPL-3.0-or-later",
    "description": "A small app to manage bank account operations.",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.5.11",
        "doctrine/persistence": "^1.3",
        "easycorp/easyadmin-bundle": "^3.0@alpha",
        "orbitale/doctrine-tools": "^0.7.5",
        "phpoffice/phpspreadsheet": "^1.10",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/string": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/web-link": "5.0.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^5.0",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "phpstan/phpstan": "^0.12.3",
        "phpstan/phpstan-deprecation-rules": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.3",
        "phpstan/phpstan-phpunit": "^0.12.1",
        "phpstan/phpstan-symfony": "^0.12.1",
        "symfony/browser-kit": "5.0.*",
        "symfony/css-selector": "5.0.*",
        "symfony/debug-bundle": "^5.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/stopwatch": "^5.0",
        "symfony/test-pack": "^1.0",
        "symfony/var-dumper": "^5.0",
        "symfony/web-profiler-bundle": "^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "compile": [
            "php bin/console doctrine:migrations:migrate --no-interaction"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.0.*"
        }
    }
}
